    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "classnames": "^2.3.1",
    "ky": "^0.27.0",
    "lodash-es": "^4.17.21",
    "mobx": "^6.3.0",
    "mobx-persist-store": "^1.0.3",
    "mobx-react-lite": "^3.2.0",
